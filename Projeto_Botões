import flet as ft
#Comando para Criação de Página em branco
def main(page:ft.Page):
 page.bgcolor ='#FFFFFF'
 page.window_width ='350'
 page.window_height ='400'
 page.window_resizable = False
 page.padding = 75
 page.window_maximizable = False

 #Botões
 btn=ft.FilledButton(text="Abrir Edge",on_click=True)
 btn.bgcolor="#6495ED"
 btn2=ft.FilledButton(text="Calculadora",on_click=True)
 btn2.bgcolor="#6495ED"

 page.add(btn,btn2)
ft.app(main)
